{% trans_default_domain 'FOSUserBundle' %}

{{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register form-signin'}}) }}
    <h2 class="form-signin-heading">{{ 'registration.submit'|trans }}</h2>
    {{ form_widget(form) }}
    <div>
        <button class="btn btn-lg btn-primary btn-block" type="submit">{{ 'registration.submit'|trans }}</button>
    </div>
{{ form_end(form) }}
